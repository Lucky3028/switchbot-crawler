    "shared": "workspace: *",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.4",
    "typesync": "^0.11.1",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "wrangler": "^3.5.1"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@holiday-jp/holiday_jp": "^2.4.0",
